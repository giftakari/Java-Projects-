package booksData;

public class Book {
String title;
String [] authors;


public static void main (String[] args ) {
	
	Book book1 = new Book();
	
	book1.title = "Learning Java";
	book1.authors = new String[] {
			"Godsgift Akari",
			"John Akari",
			"George Akari"
	                             };
	
	
      Book book2 = new Book();
      book2.title = "Queen Akari";
      book2.authors = new String[] {"Kelvin Mopho", "Inmo Mopho"};
      
      System.out.println(book1.authors);
      System.out.println(book2.authors);


}
      
}

