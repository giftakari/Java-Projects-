package realestatelistings;

public class Properties {
	private double askingPrice;
	private String propertyType;
	private double lotSize ;
	private double numBaths;
	private double numBed;
	
	public Properties( double askingPrice, String propertyType, double lotSize)
	{
		this.askingPrice = askingPrice;
		this.propertyType = propertyType;
		this.lotSize = lotSize;
		numBaths = 0;
		numBed = 0;
		
	}
	
	public Properties( double askingPrice, String propertyType, double lotSize, int numBaths,
			int numBeds)
	{
		this.askingPrice = askingPrice;
		this.propertyType = propertyType;
		this.lotSize = lotSize;
		numBaths = numBaths;
		numBed = numBed;
		
	}
	
	public String toString() 
	{
		return " Asking prices : $" + askingPrice +
				"\nPropertyType: " + propertyType +
				"\nLot Size : "+ lotSize + 
				"\nNumber of baths: " + numBaths +
				"\nNumber of bedrooms: " + numBed;
		
	}
	

}
